{
  "short_name": "Tabs Manager",
  "name": "Tabs Manager Extension Tool",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs", "storage"],
  "manifest_version": 2,
  "author": "Peter Lin",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Tabs Manager"
  },
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
